{
  "resources": {
    "includes": [
      {
        "pattern": "\\QMETA-INF/services/org.slf4j.spi.SLF4JServiceProvider\\E",
        "condition": {
          "typeReachable": "org.slf4j.LoggerFactory"
        }
      },
      {
        "pattern": "\\Qorg/slf4j/impl/StaticLoggerBinder.class\\E",
        "condition": {
          "typeReachable": "org.slf4j.LoggerFactory"
        }
      },
      {
        "condition": {
          "typeReachable": "org.apache.log4j.xml.Log4jEntityResolver"
        },
        "pattern": "\\Qorg/apache/log4j/xml/log4j.dtd\\E"
      },
      {
        "condition": {
          "typeReachable": "org.apache.log4j.helpers.Loader"
        },
        "pattern": "\\Qlog4j.properties\\E"
      }
    ]
  },
  "bundles": [
  ]
}